<div class="container">
  <div class="row">
    <h1><%= t :categories %></h1>
    <table class="table table-bordered">
      <tr>
        <th><%= t :id %></th>
        <th><%= t :category_name %></th>
        <th><%= t :max_question %></th>
        <th><%= t :max_time %></th>
      </tr>
      <%= render @categories %>
    </table>
    <%= will_paginate %>
  </div>
</div>
